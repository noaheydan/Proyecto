gBA;AAjBA,GANA;AAyBA,EAAA,OAAA;AAAA,2EAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,mBAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,cAAA,CAAA,OAAA,CAAA,SAAA,CAAA,CAAA;;AADA,oBAEA,KAAA,OAAA,IAAA,IAFA;AAAA;AAAA;AAAA;;AAGA,mBAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AAHA;AAAA;;AAAA;AAAA,oBAIA,KAAA,OAAA,CAAA,IAAA,IAAA,SAJA;AAAA;AAAA;AAAA;;AAKA,mBAAA,OAAA,CAAA,IAAA,CAAA,UAAA;AALA;AAAA;;AAAA;AAAA;AAAA;AAAA,qBAQA,KAAA,KAAA,CAAA,GAAA,4BAAA,KAAA,OAAA,CAAA,EAAA,EARA;;AAAA;AAQA,cAAA,GARA;AASA,mBAAA,WAAA,GAAA,GAAA,CAAA,IAAA;AATA;AAAA;;AAAA;AAAA;AAAA;AAWA,cAAA,OAAA,CAAA,GAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,KAzBA;AAwCA,EAAA,UAAA,EAAA;AACA,IAAA,OAAA,EAAA;AADA;AAxCA,CAAA","sourcesContent":["<template>\n  <v-container>\n    <NavbarS :s_name=\"student.name\" />\n\n    <v-alert text v-model=\"alert.show\" :type=\"alert.type\" dismissible>{{alert.message}}</v-alert>\n    <h1 class=\"font-weight-light\">Soluciones Disponibles</h1>\n    <v-row justify=\"center\">\n      <v-card class=\"ma-3\" max-width=\"344\" v-for=\"course in coursesList\" :key=\"course.id_c\">\n        <v-img\n          src=\"https://www.esa-automation.com/wp-content/uploads/2016/02/IoT.jpg\"\n          height=\"200px\"\n          class=\"white--text align-end\"\n        >\n          <v-card-title >{{course.c_name}}</v-card-title>\n          <v-card-subtitle class=\"white--text\">\n            {{course.p_name}}\n          </v-card-subtitle>\n        </v-img>\n        <v-card-text>\n          {{course.c_description}}\n          <!-- <div>\n            <b>E-mail:</b>\n            {{course.p_email}}\n          </div> -->\n        </v-card-text>\n        <v-card-actions>\n          <v-btn color=\"purple\" dark absolute right @click=\"joinIn(course.id_c)\">Agregar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport NavbarS from \"@/components/NavbarS\";\n\nexport default {\n  data: () => ({\n    student: {},\n    coursesList: [],\n    alert: { show: false }\n  }),\n  methods: {\n    async joinIn(id_c) {\n      try {\n        const res = await this.axios.post(\n          `/student/course/${id_c}`,\n          this.student\n        );\n        const index = this.coursesList.findIndex(c => c.id_c == id_c);\n        this.coursesList.splice(index, 1);\n        this.alert = {\n          show: true,\n          type: \"success\",\n          message: res.data.message\n        };\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  },\n  created: async function() {\n    this.student = JSON.parse(sessionStorage.getItem(\"session\"));\n    if (this.student == null) {\n      this.$router.push(\"/\");\n    } else if (this.student.role != \"student\") {\n      this.$router.push(\"/profile\");\n    } else {\n      try {\n        const res = await this.axios.get(`/student/courses/${this.student.id}`);\n        this.coursesList = res.data;\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  },\n  components: {\n    NavbarS\n  }\n};\n</script>"],"sourceRoot":"src/views/student"}]}